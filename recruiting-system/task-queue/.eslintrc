{
  "env": {
    "node": true
  },
  "rules": {
    "camelcase": 2,
    "curly": 2,
    "brace-style": [2, "1tbs"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "object-curly-spacing": [2, "never"],
    "computed-property-spacing": [2, "never"],
    "array-bracket-spacing": 2,
    "eqeqeq": 2,
    "indent": [2, 2],
    "no-magic-numbers": 2
  },
  "parser": "babel-eslint"
}